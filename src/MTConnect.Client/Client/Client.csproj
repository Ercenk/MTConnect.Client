<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>MTConnect.Client</PackageId>
    <Authors>Ercenk Keresteci</Authors>
    <Product>MTConnect.Client</Product>
    <Description>An MTConnect client implementation based on the code generated from the swagger spec for MTConect 1.3.1.</Description>
    <PackageLicenseUrl>https://github.com/Ercenk/MTConnect.Client/blob/master/License.md</PackageLicenseUrl>
    <Copyright>Copyright (c) 2018, Ercenk Keresteci</Copyright>
    <PackageProjectUrl>https://github.com/Ercenk/MTConnect.Client</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Ercenk/MTConnect.Client</RepositoryUrl>
    <PackageTags>MTConnect</PackageTags>
    <Version>1.0.5</Version>
    <PackageReleaseNotes>Fixed a bug on the YAML spec</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Rest.ClientRuntime" Version="2.3.11" />
  </ItemGroup>

</Project>
